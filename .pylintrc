# pylint config
[FORMAT]
good-names=r
[MESSAGES CONTROL]
disable=fixme,
        duplicate-code,
        import-error,
        line-too-long,
        missing-class-docstring,
        missing-function-docstring,
        missing-module-docstring,
        too-few-public-methods,
        too-many-branches,
        too-many-locals,
        too-many-return-statements,
        too-many-statements,
        use-list-literal,
        use-dict-literal
